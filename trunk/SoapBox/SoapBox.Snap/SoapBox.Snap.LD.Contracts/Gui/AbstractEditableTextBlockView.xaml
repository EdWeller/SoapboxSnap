    xmlns:contracts="clr-namespace:SoapBox.Snap;assembly=SoapBox.Snap.Contracts"
    x:Class="SoapBox.Snap.LD.AbstractEditableTextBlockView">

    <DataTemplate DataType="{x:Type local:AbstractEditableTextBlock}">
